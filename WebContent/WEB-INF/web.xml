		<servlet-name>EtudiantHandler</servlet-name>
		<url-pattern>/EtudiantHandler</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<description></description>
		<servlet-name>EmpruntHandler</servlet-name>
